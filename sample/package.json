    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ts-transformer-classname": "^2.0.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not op_mini all",
    "Firefox >= 75",
    "Chrome >= 80",
    "Firefox >= 75",
    "safari >= 12"
  ],
  "scripts": {
    "start": "concurrently \"yarn watch-ts\" \"yarn watch-js\"",
    "start-server": "concurrently \"yarn watch-ts\" \"yarn run-ssr\"",
    "watch-ts": "ttsc --incremental -w",
    "watch-js": "vite",
    "run-ssr": "ttsc --incremental && yarn build-server && node build/server/index.js --experimental-fetch",
    "build": "rm -rf build && ttsc && yarn build-client && yarn build-server",
    "build-client": "vite build --outDir build/client",
    "build-server": "esr building/ssr-compiler.ts",
    "stylelint": "stylelint 'assets/scss/**/*.scss'",
    "eslint": "eslint src --fix --ext .ts,.tsx",
    "typescript": "tsc --project tsconfig.json --noEmit",
    "test": "jest --config jest.config.js",
    "coverage": "jest --config jest.config.js --coverage --watchAll=false"
  },
  "packageManager": "yarn@3.3.1"
}
